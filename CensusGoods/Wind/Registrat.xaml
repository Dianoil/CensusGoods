<Window x:Class="CensusGoods.Wind.Registrat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CensusGoods.Wind"
        mc:Ignorable="d"
        Title="Registrat" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Census goods"/>
        <TextBox Text="Номер телефона"
                 Grid.Row="1"
                 Grid.Column="1"/>
        <TextBox Text="E-mail"
                 Grid.Row="2"
                 Grid.Column="1"/>
        <TextBox Text="Пароль"
                 Grid.Row="3"
                 Grid.Column="1"/>
        <Label Content="Валюта"
               Grid.Row="5"/>
        <ComboBox Grid.Row="5"
                  Grid.Column="1"/>
        <Label Content="Тип магазина"
               Grid.Row="6"/>
        <ComboBox Grid.Row="6"
                  Grid.Column="1"/>
        <TextBox Text="Название фирмы"
                 Grid.Row="7"
                 Grid.Column="1"/>
        <TextBox Text="ИНН"
                 Grid.Row="8"
                 Grid.Column="1"/>
        <TextBox Text="ОГРН"
                 Grid.Row="9"
                 Grid.Column="1"/>

        <Button Content="Регистрация"
                Grid.Column="1"
                Grid.Row="10"
                x:Name="regist"
                Click="regist_Click"/>
    </Grid>
</Window>
