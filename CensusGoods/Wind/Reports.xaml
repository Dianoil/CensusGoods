<Window x:Class="CensusGoods.Wind.Reports"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CensusGoods.Wind"
        mc:Ignorable="d"
        Title="Reports" Height="1080" Width="1920">
    <Grid>
        <!--Переходное окно на отчеты-->
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Census goods"/>
        <Button Content="Тех. поддержка"
                x:Name="tehsupport"
                Click="tehsupport_Click"
                Grid.Column="1"/>
        <ComboBox Grid.Row="1"
                  Grid.Column="1"/>
        <Button Content="Выбрать"
                Grid.Column="1"
                Grid.Row="2"
                x:Name="reporttxt"
                Click="reporttxt_Click"/>
    </Grid>
</Window>
